package model;

public class Person {
	private String name;
	private Ticket ticket;
	public String getName() {
		return name;
	}
	/*
	 * 스프링 컨테이너가 Person의 name 을 
	 * setName() 메서드로 주입한다. 
	 * : DI (Dependency injection)
	 */
	public void setName(String name) {
		this.name = name;
	}
	public Ticket getTicket() {
		return ticket;
	}
	public void setTicket(Ticket ticket) {
		this.ticket = ticket;
	}
	
}
