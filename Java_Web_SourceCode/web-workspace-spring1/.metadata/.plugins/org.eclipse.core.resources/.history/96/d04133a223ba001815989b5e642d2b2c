<mapper namespace="customer">
	<select id="findCustomerById" resultType="customerVO">
		select id,name,address from spring_customer
		where id=#{value}
	</select>
	<select id="findCustomerByAddress" resultType="customerVO">
		select id,name,address from spring_customer
		where id=#{value}
	</select>
</mapper>