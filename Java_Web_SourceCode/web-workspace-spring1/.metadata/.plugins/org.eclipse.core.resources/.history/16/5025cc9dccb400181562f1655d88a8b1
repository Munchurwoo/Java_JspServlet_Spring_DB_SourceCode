package test;

import org.springframework.context.support.ClassPathXmlApplicationContext;

import model.AccountDAO;
import model.AccountServiceImpl;

public class TestIOC {
	public static void main(String[] args) {
		ClassPathXmlApplicationContext factory = new ClassPathXmlApplicationContext("AccountService");
		AccountDAO Service = (AccountDAO)factory.getBean("accountDAO");
		
		System.out.println("검색결과 :"+Service.findAccountById("1"));
		factory.close();
	}
}
